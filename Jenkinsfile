pipeline {
    
    agent any
    
    stages {
        
        stage('Init'){
            steps{
                echo 'Init'
                echo '*********'
            }
        }
        stage('yarn install'){
            steps{
                echo 'yarn install'
                echo '*********'
            }
        }
        stage('yarn build'){
            steps{
                echo 'yarn build'
                echo '*********'
            }
        }
    }
}